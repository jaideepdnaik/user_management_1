<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/style.css">
  <title>Users</title>
</head>
<body>
  <h1>All Users</h1>
  <ul>
    <% data.forEach(user => { %>
      <li>
        <%= user.username %> - <%= user.email %>
        <!-- <a href="/user/<%= user.id %>/edit">Edit</a> -->
        <a href="/user/<%= user.id %>/delete">Delete</a>
      </li>
    <% }) %>
  </ul>
  <a href="/">Home</a>
</body>
</html>
